# NexusGreen Production Environment Configuration
# World-Class Solar Energy Management Platform

# Application Configuration
VITE_APP_NAME=NexusGreen
VITE_APP_VERSION=6.0.0
VITE_APP_DESCRIPTION=World-Class Solar Energy Management Platform
VITE_ENVIRONMENT=production

# API Configuration
VITE_API_URL=https://nexus.gonxt.tech/api
VITE_WS_URL=wss://nexus.gonxt.tech/ws

# Company Configuration
VITE_COMPANY_NAME=NexusGreen Energy Solutions
VITE_COMPANY_REG=NGES-2024-001
VITE_PPA_RATE=0.12

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_EXPORT=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_MODERN_UI=true

# Security Configuration
VITE_ENABLE_CSP=true
VITE_SECURE_COOKIES=true

# Performance Configuration
VITE_ENABLE_SERVICE_WORKER=true
VITE_ENABLE_COMPRESSION=true
VITE_CACHE_TTL=300000

# Build Configuration
VITE_BUILD_SOURCEMAP=false
VITE_BUILD_MINIFY=true
VITE_BUILD_TARGET=esnext

# Monitoring and Logging
VITE_ENABLE_ERROR_REPORTING=true
VITE_LOG_LEVEL=warn

# External Services (replace with actual keys in production)
VITE_MAPS_API_KEY=your_maps_api_key_here
VITE_WEATHER_API_KEY=your_weather_api_key_here

# Database Configuration (for backend)
DATABASE_URL=postgresql://nexusgreen:nexusgreen@localhost:5432/nexusgreen
SOLAX_SYNC_INTERVAL_MINUTES=60

# Build Configuration (handled by Vite)
GENERATE_SOURCEMAP=false